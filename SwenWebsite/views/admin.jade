extends ../node_modules/jade-bootstrap/layouts/blog

block body
 +navbar("Colenso Project", "dropdown_menu")
  +nav_item("/") Home  
  +nav_item("/browse") Browse
  +nav_item("/admin","active") Admin
  +nav_item_dropdown("/search")(label = "Search")
   +nav_item("/search","active") Search
   +nav_item("/xquery") XQuery
   +nav_item("/xpath") XPath
 
 .container   
  h2 Add new letter
  form.form-horizontal(action="admin/new", method="post", enctype="multipart/form-data")
   label.control-label(for="file") Document to upload
   input(type="file", name="newDoc", required="true")
   br
   button.btn.btn-default(type="submit", name="submit") Upload Document

      


 
